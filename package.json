{
  "name": "simple-numbers",
  "version": "1.0.0",
  "description": "A small, fast JavaScript library for arbitrary-precision decimal arithmetic in javascript notation.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "test": "jest --config ./jest.config.ts  --passWithNoTests __tests__/op.test.ts",
    "test-watch": "jest --config ./jest.config.ts  --passWithNoTests --watchAll",
    "dev": "nodemon --watch src/ --ext ts --exec 'tsc && node dist/index.js'",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "keywords": [
    "arbitrary",
    "precision",
    "arithmetic",
    "big",
    "number",
    "decimal",
    "float",
    "biginteger",
    "bigdecimal",
    "bignumber",
    "bigint",
    "bignum"
  ],
  "author": {
    "name": "Andranik Ghevondyan",
    "url": "https://github.com/AndoGhevian"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndoGhevian/SimpleNumbers.git"
  },
  "bugs": {
    "url": "https://github.com/AndoGhevian/SimpleNumbers/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.4.3",
    "nodemon": "^2.0.4",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@jsep-plugin/assignment": "^1.1.0",
    "@types/big.js": "^6.1.2",
    "big.js": "^6.1.1",
    "jsep": "^1.2.0"
  }
}
